<h1>All Users</h1>
<ul>
  <% users.forEach(user => { %>
    <li>
      <%= user.username %> - <%= user.email %>
      <% if (!user.isAdmin) { %>
        <form action="/admin/users/<%= user._id %>/delete" method="POST" style="display:inline;">
          <button>Delete</button>
        </form>
      <% } else { %>
        (Admin)
      <% } %>
    </li>
  <% }) %>
</ul>
